@model IEnumerable<CarModel>
@{
    ViewData["Title"] = "Table 'CarModels'";
}
<h1>@ViewData["Title"]</h1>

<p align="center">
    <table border="1">
        <tr>
            <td>Brand</td>
            <td>Name</td>
            <td>Description</td>
        </tr>
        @foreach (var genre in Model)
        {
            <tr>
                <td>@genre.Brand</td>
                <td>@genre.Name</td>
                <td>@genre.Description</td>
            </tr>
        }
    </table>
</p>
